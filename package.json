{
  "name": "ndse-homeworks",
  "version": "1.0.0",
  "description": "Homeworks assignments for course the Backend Node.js developer",
  "main": "server.js",
  "engines": {
    "node": ">=14.17.4",
    "npm": ">=6.14.14"
  },
  "scripts": {
    "test": "jest --passWithNoTests",
    "link": "npm link",
    "unlink": "npm unlink -g",
    "eslint": "eslint . --fix",
    "build-homeworks": "./node_modules/.bin/tsc",
    "compile": "tsc --noEmit",
    "validate": "npm-run-all compile test eslint",
    "prepare": "husky install",
    "homework-1-2-1": "npm run build-homeworks && npm link && get-date"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Pelmenya/ndse-homeworks.git"
  },
  "keywords": [],
  "author": "Dmitry Lyapin",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Pelmenya/ndse-homeworks/issues"
  },
  "homepage": "https://github.com/Pelmenya/ndse-homeworks#readme",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "4.29.0",
    "@typescript-eslint/parser": "4.29.0",
    "eslint": "7.32.0",
    "eslint-config-airbnb-typescript": "12.3.1",
    "eslint-formatter-pretty": "4.1.0",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-import": "2.23.4",
    "eslint-plugin-jest": "24.4.0",
    "eslint-plugin-promise": "5.1.0",
    "husky": "7.0.1",
    "jest": "27.0.6",
    "npm-run-all": "4.1.5",
    "typescript": "4.8.3"
  },
  "dependencies": {
    "yargs": "17.5.1"
  }
}
